 6)package main
import "fmt"
func main() {
sum := 0
for i := 1; i < 5; i++ {
    sum += i
}
fmt.Println(sum)
}

10

Program exited.

7)package main
import "fmt"
func main() {
n := 1
for n < 5 {
    n *= 2
    n++
}
fmt.Println(n)
}

8

Program exited.

8)package main
import "fmt"
func main() {
strings := []string{"hello", "world"}
for i, s := range strings {
    fmt.Println(i, s)
}
}

0 hello
1 world

Program exited.

9)package main
import "fmt"
func main() {
sum := 0
for i := 1; i < 5; i++ {
    if i%2 != 0 { 
        continue
    }
    sum += i
}
fmt.Println(sum)
}


6

Program exited.

10)package main
import "fmt"
func main() {
sum := 0
for i := 1; i < 5; i++ {
    if i%1 != 0 { 
        continue
    }
    sum += i
}
fmt.Println(sum)
}

10

Program exited.
